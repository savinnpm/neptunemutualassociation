{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es2016",
      "jsx": "preserve",
      "checkJs": true,
      "baseUrl": ".",
      "paths": {
        "@/icons/*": [
          "src/icons/*"
        ],
        "@/common/*": [
          "src/common/*"
        ],
        "@/pages/*": [
          "pages/*"
        ],
        "@/src/*": [
          "src/*"
        ],
      }
    },
    "exclude": [
      "node_modules",
      "**/node_modules/*",
      "out"
    ]
  }